// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "Other": true,
        "**/*.lnk": true,
        "**/*.blg": true,
        "**/*.aux": true,
        "**/*.bbl": true,
        "**/*.lof": true,
        "**/*.fls": true,
        "**/*.dvi": true,
        "**/*.lot": true,
        "**/*.toc": true,
        "**/*.out": true,
        "**/*.fdb_latexmk": true
    },

    "editor.minimap.enabled": false,

    "gitlens.blame.line.enabled": false
}