Cell[BoxData[{
 RowBox[{
  RowBox[{"EqTeukolskyAngular", "=", 
   RowBox[{
    RowBox[{
     SubscriptBox["\[PartialD]", "x"], 
     RowBox[{"(", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{"1", "-", 
         SuperscriptBox["x", "2"]}], ")"}], " ", 
       RowBox[{
        SuperscriptBox["S", "\[Prime]",
         MultilineFunction->None], "[", "x", "]"}]}], ")"}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"c", " ", "x"}], ")"}], "2"], "-", 
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"m", "+", 
           RowBox[{"s", " ", "x"}]}], ")"}], "2"], 
        RowBox[{"1", "-", 
         SuperscriptBox["x", "2"]}]], "+", "s", "-", 
       RowBox[{"2", " ", "c", " ", "s", " ", "x"}], "+", "A"}], ")"}], " ", 
     RowBox[{"S", "[", "x", "]"}]}]}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ansatzS", "=", 
   RowBox[{"{", 
    RowBox[{"S", "\[Rule]", 
     RowBox[{"Function", "[", 
      RowBox[{"x", ",", 
       RowBox[{
        SuperscriptBox["\[ExponentialE]", 
         RowBox[{"c", " ", "x"}]], 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "+", "x"}], ")"}], "km"], " ", 
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"1", "-", "x"}], ")"}], "kp"], 
        RowBox[{"F", "[", "x", "]"}]}]}], "]"}]}], "}"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"Rule\[Kappa]pm", "=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"kp", "\[Rule]", 
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"s", "+", "m"}], "]"}], "/", "2"}]}], ",", 
     RowBox[{"km", "\[Rule]", 
      RowBox[{
       RowBox[{"Abs", "[", 
        RowBox[{"s", "-", "m"}], "]"}], "/", "2"}]}]}], "}"}]}], 
  ";"}]}], "Input",
