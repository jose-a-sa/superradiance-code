        SuperscriptBox["S", "\[Prime]",
         MultilineFunction->None], "[", "x", "]"}]}], ")"}]}], "+", 
    RowBox[{
     RowBox[{"(", 
      RowBox[{
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"c", " ", "x"}], ")"}], "2"], "-", 
       FractionBox[
        SuperscriptBox[
         RowBox[{"(", 
          RowBox[{"m", "+", 
           RowBox[{"s", " ", "x"}]}], ")"}], "2"], 
        RowBox[{"1", "-", 
         SuperscriptBox["x", "2"]}]], "+", "s", "-", 
       RowBox[{"2", " ", "c", " ", "s", " ", "x"}], "+", "A"}], ")"}], " ", 
