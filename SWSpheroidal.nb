      InterpretationFunction->(RowBox[{
         RowBox[{"Inactive", "[", "Sum", "]"}], "[", 
         RowBox[{#, ",", 
           RowBox[{"{", 
             RowBox[{#2, ",", #3, ",", #4}], "}"}]}], "]"}]& )], 
     "\[RuleDelayed]", 
     TemplateBox[{RowBox[{
         SuperscriptBox["z", "p"], " ", 
         RowBox[{"a", "[", 
           RowBox[{
             RowBox[{"2", " ", "p"}], "-", "v"}], "]"}], " ", 
         RowBox[{"(", 
           RowBox[{"b", "/.", "\[VeryThinSpace]", 
             RowBox[{"{", 
               RowBox[{"p", "\[Rule]", 
                 RowBox[{
                   RowBox[{"2", " ", "p"}], "-", "v"}]}], "}"}]}], ")"}]}],
       "p",RowBox[{"i", "-", "p", "+", "v"}],"\[Infinity]"},
      "InactiveSum",
      DisplayFunction->(RowBox[{
